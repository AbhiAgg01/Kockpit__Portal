<div class="modal fade" id="logoutModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Warning!</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <img src="~/assets/Group 5529.png" alt="">
                </button>
            </div>
            <div class="modal-body">
                <img src="~/assets/Group 6261.svg" alt="">
                <p>Are you sure you want to <br>logout?</p>
            </div>
            <div class="modal-footer">
                <a type="button" class="button btn-logout" href="@Url.Action("Logout","Login")"  onclick="expireCookie()">Logout</a>
            </div>
        </div>
    </div>
</div>
<script src="~/js/cookiemanager.js"></script>