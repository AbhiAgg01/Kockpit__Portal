                        </g>
                    </svg>
                </div>
                <div class="text">
                    <p class="card-text-heading"><a asp-action="Index" asp-controller="Employee">Total Support Person</a></p>
                    <p class="card-text-count">@Model.TotalEmployees</p>
                </div>
            </div>
            <div class="gradient-card">
                <div class="icon">
                    <svg class="iconify third" id="interview_1_" data-name="interview (1)" xmlns="http://www.w3.org/2000/svg" width="26.807" height="26.807" viewBox="0 0 26.807 26.807">
