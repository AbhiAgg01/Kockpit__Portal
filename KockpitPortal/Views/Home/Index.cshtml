@model KockpitPortal.Models.SuperAdminDashboard
@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
    var nClient = 0;
    var nSupportManager = 0;
    var nSupportRepresentative = 0;
}



<div class="main">
