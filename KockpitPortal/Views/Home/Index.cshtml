                        <path id="Path_15988" data-name="Path 15988" d="M266.264,80.332h-8.412a.785.785,0,0,0,0,1.571h8.412a.785.785,0,0,0,0-1.571Zm0,0" transform="translate(-243.607 -76.126)" fill="#fff" />
                        <path id="Path_15989" data-name="Path 15989" d="M266.264,144.6h-8.412a.785.785,0,0,0,0,1.571h8.412a.785.785,0,0,0,0-1.571Zm0,0" transform="translate(-243.607 -137.031)" fill="#fff" />
                        <path id="Path_15990" data-name="Path 15990" d="M262.058,208.867h-4.206a.785.785,0,0,0,0,1.571h4.206a.785.785,0,0,0,0-1.571Zm0,0" transform="translate(-243.607 -197.932)" fill="#fff" />
                    </svg>
                </div>
                <div class="text">
                    <p class="card-text-heading"><a asp-action="Index" asp-controller="PlanRequest">License Request</a> </p>
                    <p class="card-text-count">@Model.LicenseRequests</p>
                </div>
            </div>
        </div>
        <div class="admin-dashboard-secondary-grid">
            <div class="secondary-card">
                <div class="icon">
                    <span class="iconify" data-icon="icomoon-free:profile"></span>
                </div>
                <div class="text">
                    <p class="card-text-heading">Total License Sold</p>
                    <p class="card-text-count">@Model.TotalLicenses</p>
                </div>
            </div>
            <div class="secondary-card">
                <div class="icon">
                    <span class="iconify" data-icon="icomoon-free:profile"></span>
                </div>
                <div class="text">
                    <p class="card-text-heading">Active License</p>
                    <p class="card-text-count">@Model.ActiveLicenses</p>
                </div>
            </div>
            <div class="secondary-card">
                <div class="icon">
                    <span class="iconify" data-icon="icomoon-free:profile"></span>
                </div>
                <div class="text">
                    <p class="card-text-heading">Inactive License</p>
                    <p class="card-text-count">@Model.InActiveLicenses</p>
                </div>
            </div>
            <div class="secondary-card">
                <div class="icon">
                    <span class="iconify" data-icon="icomoon-free:profile"></span>
                </div>
                <div class="text">
                    <p class="card-text-heading">Expired License</p>
                    <p class="card-text-count">@Model.ExpiredLicenses</p>
                </div>
            </div>
        </div>

        <div class="table-grid">
            <div class="table-wrapper">
                <div class="table-header">
                    Clients
                </div>
                <table>
                    <thead>

                        <tr>
                            <th>S.No</th>
                            <th>Company</th>
                            <th>E-Mail</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.ClientList != null && Model.ClientList.Count > 0)
                        {
                            foreach (var item in Model.ClientList)
                            {
                                <tr>
                                    <td>@(nClient += 1)</td>
                                    <td>
                                        <div class="icon"></div>
                                        <p>@item.CompanyName</p>
                                    </td>
                                    <td>@item.EmailId</td>
                                </tr>

                            }
                        }
                    </tbody>
                </table>
            </div>
            <div class="table-wrapper">
                <div class="table-header">
                    Support Managers
                </div>
                <table>
                    <thead>
                        <tr>
                            <th>S.No</th>
                            <th>Company</th>
                            <th>E-Mail</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.SupportManager != null && Model.SupportManager.Count > 0)
                        {
                            foreach (var item in Model.SupportManager)
                            {
                                <tr>
                                    <td>@(nSupportManager += 1)</td>
                                    <td>
                                        <div class="icon"></div>
                                        <p>@item.CompanyName</p>
                                    </td>
                                    <td>@item.EmailId</td>
                                </tr>

                            }
                        }
                    </tbody>
                </table>
            </div>
            <div class="table-wrapper">
                <div class="table-header">
                    Support Representatives
                </div>
                <table>
                    <thead>
                        <tr>
                            <th>S.No</th>
                            <th>Company</th>
                            <th>E-Mail</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.SupportRepresentative != null && Model.SupportRepresentative.Count > 0)
                        {
                            foreach (var item in Model.SupportRepresentative)
                            {
                                <tr>
                                    <td>@(nSupportRepresentative += 1)</td>
                                    <td>
                                        <div class="icon"></div>
                                        <p>@item.CompanyName</p>
                                    </td>
                                    <td>@item.EmailId</td>
                                </tr>

                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

