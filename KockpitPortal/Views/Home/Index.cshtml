                            <path id="Path_15981" data-name="Path 15981" d="M27.729,102.457A2.729,2.729,0,1,0,25,99.729,2.732,2.732,0,0,0,27.729,102.457Zm0-3.969a1.24,1.24,0,1,1-1.24,1.24A1.242,1.242,0,0,1,27.729,98.488Z" transform="translate(-23.76 -60.825)" fill="#fff" />
                        </g>
                    </svg>

                </div>
                <div class="text">
                    <p class="card-text-heading"> <a asp-action="Index" asp-controller="UserManagement">Total Clients</a> </p>
                    <p class="card-text-count"> @Model.TotalClients</p>
                </div>
            </div>
            <div class="gradient-card">
                <div class="icon">
                    <svg class="iconify second" xmlns="http://www.w3.org/2000/svg" width="17.381" height="24.11" viewBox="0 0 17.381 24.11">
                        <g id="businessman_1_" data-name="businessman (1)" transform="translate(-4.25 -1.25)">
